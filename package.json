{
  "publisher": "wChenonly",
  "name": "easy-gerrit",
  "displayName": "easy-gerrit",
  "version": "0.0.2",
  "description": "方便在vscode中直接提交代码到gerrit",
  "repository": {
    "type": "git",
    "url": "https://github.com/wChenonly/easy-gerrit"
  },
  "categories": [
    "SCM Providers"
  ],
  "main": "./out/extension.js",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "issue": {
    "url": "https://github.com/wChenonly/easy-gerrit/issues"
  },
  "activationEvents": [
    "onCommand:easy-gerrit.editingCommit",
    "onCommand:easy-gerrit.pushCode"
  ],
  "contributes": {
    "commands": [
      {
        "command": "easy-gerrit.editingCommit",
        "title": "editingCommit",
        "icon": {
          "dark": "/images/dark-editing-commit.svg",
          "light": "/images/light-editing-commit.svg"
        }
      },
      {
        "command": "easy-gerrit.pushCode",
        "title": "pushCode",
        "icon": {
          "dark": "/images/dark-push-code.svg",
          "light": "/images/light-push-code.svg"
        }
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "easy-gerrit.editingCommit",
          "group": "navigation"
        },
        {
          "when": "scmProvider == git",
          "command": "easy-gerrit.pushCode",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "build": "vsce package",
    "publish": "vsce package && vsce publish",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint . && yarn run format",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.2",
    "@types/vscode": "^1.80.0",
    "eslint": "^8.45.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "@vscode/vsce": "^2.19.0"
  }
}
